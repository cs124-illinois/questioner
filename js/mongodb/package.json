{
  "name": "@cs124/questioner-mongodb",
  "version": "2025.7.0",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "checker": "yarn depcheck && yarn prettier && yarn eslint && yarn tsc && yarn build",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*,rimraf --ignore-dirs=dist",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prettier": "prettier -w --cache --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "prepublish": "rimraf dist && tsc",
    "start": "tsc -w",
    "tsc": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@cs124/questioner-types": "2025.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "depcheck": "1.4.7",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "mongodb": "6.18.0",
    "npm-check-updates": "18.0.2",
    "prettier": "3.6.2",
    "prettier-package-json": "gchallen/prettier-package-json#2023.8.0",
    "prettier-plugin-organize-imports": "4.2.0",
    "rimraf": "6.0.1",
    "runtypes": "7.0.4",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}

  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "checker": "yarn depcheck && yarn prettier && yarn eslint && yarn tsc",
    "clean": "rimraf dist",
    "depcheck": "depcheck --ignores=prettier-plugin-*,rimraf --ignore-dirs=dist",
    "eslint": "eslint src/**",
    "ncu": "ncu",
    "prettier": "prettier -w --cache --plugin=prettier-plugin-organize-imports . && prettier-package-json --write",
    "prepublish": "rimraf dist && tsc",
    "start": "tsc -w",
    "tsc": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "types": "dist/index.d.ts",
  "peerDependencies": {
    "@cs124/jeed-types": ">= 2021",
    "runtypes": ">= 6"
  },
